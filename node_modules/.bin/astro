#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liutingkai/blogTTTK/node_modules/.pnpm/astro@5.16.4_@types+node@24.10.1_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.8_3921b9020476c2736ccd7cff93b55623/node_modules/astro/node_modules:/Users/liutingkai/blogTTTK/node_modules/.pnpm/astro@5.16.4_@types+node@24.10.1_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.8_3921b9020476c2736ccd7cff93b55623/node_modules:/Users/liutingkai/blogTTTK/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/liutingkai/blogTTTK/node_modules/.pnpm/astro@5.16.4_@types+node@24.10.1_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.8_3921b9020476c2736ccd7cff93b55623/node_modules/astro/node_modules:/Users/liutingkai/blogTTTK/node_modules/.pnpm/astro@5.16.4_@types+node@24.10.1_jiti@1.21.7_lightningcss@1.29.3_rollup@2.79.2_sass@1.8_3921b9020476c2736ccd7cff93b55623/node_modules:/Users/liutingkai/blogTTTK/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
